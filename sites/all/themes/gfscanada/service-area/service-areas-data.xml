	<distributions>
		<distribution id="restaurants" name="Restaurants, Healthcare, Education and Other Foodservice">
			<messages>
				<message pattern="Canada$" id="canada-standard-distribution" />
				<message pattern=".*" id="us-standard-distribution" />
			</messages>
			<polygonGroups fill-color="FF0000" fill-opacity="20" stroke-color="AA4040" stroke-opacity="50" stroke-weight="3">
				<polygonGroup pattern="USA$" id="us-standard-distribution" />
				<polygonGroup pattern="((AB|BC|MB|NB|NL|NS|ON|PE|QC|SK|YT|NT), Canada|^Canada)$" id="canada-standard-distribution" />
			</polygonGroups>
		</distribution>
		<distribution id="chain-operators" name="Chain Operators">
			<messages>
				<message pattern=".*" id="us-chain-distribution" /> 
			</messages>
			<polygonGroups fill-color="000000" fill-opacity="10" stroke-color="000000" stroke-opacity="35" stroke-weight="3">
				<polygonGroup pattern="USA$" id="us-chain-distribution" />
				<polygonGroup pattern="((AB|BC|MB|NB|NL|NS|ON|PE|QC|SK), Canada|^Canada)$" id="canada-standard-distribution" />
			</polygonGroups>
		</distribution>
	</distributions>
